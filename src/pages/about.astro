---
import { getEntry, render } from 'astro:content';
import AboutPage from '../layouts/AboutPage.astro';

const about = await getEntry('about', 'about');

if (!about) {
  throw new Error('`about/about.md` が見つかりません。');
}

const { Content } = await render(about);
---

<AboutPage {...about.data}>
  <Content />
</AboutPage>
